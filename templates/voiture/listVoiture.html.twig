<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<style>
    body{
        font-family: sans-serif
    }
</style>
<body>
<h1>Liste Des Voitures</h1>
<table border="1">
    <tr>
        <td>Serie</td>
        <td>Date MC</td>
        <td>Modele</td>
    </tr>
    {% for v in voitures %}
    <tr>
        <td>{{ v.serie }}</td>
        <td>{{ v.DateMiseEnMarche | date(null, "Europe/Paris") }}</td>
        <td>{{ v.modele }}</td>
    </tr>
    {% endfor %}
</table>


</body>
</html>